<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>qttabbar-langfiles</id>
    <version>1.5.0.20130713</version>
    <title>QTTabBar language files</title>
    <owners>chocolatey</owners>
    <authors>QTTabBar translation team</authors>
    <licenseUrl>http://www.gnu.org/licenses/gpl.html</licenseUrl>
    <projectUrl>http://qttabbar.sourceforge.net/</projectUrl>
