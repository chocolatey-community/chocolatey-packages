<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>truecrypt-langfiles</id>
		<version>7.1.1</version>
		<title>TrueCrypt language files</title>
		<authors>Various (see http://www.truecrypt.org/localizations)</authors>
		<projectUrl>http://www.truecrypt.org/localizations</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
